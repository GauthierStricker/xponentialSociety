  <%= link_to root_path, class: "navbar-wagon-brand" do %>
    <%= image_tag "xponential-society-logo.png", height: 70 %>
  <% end %>

  <!-- Right Navigation -->
  <div class="navbar-wagon-right hidden-xs hidden-sm">
      <%= link_to "Contribute", "#", class: "navbar-wagon-item navbar-wagon-link" %>
